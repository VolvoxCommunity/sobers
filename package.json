{
  "name": "sobriety-waypoint",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "start": "expo start",
    "start:clean": "pnpm clean:metro && expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:android": "expo export --platform android",
    "build:ios": "expo export --platform ios",
    "lint": "expo lint",
    "clean:metro": "rm -rf .expo node_modules/.cache",
    "clean:all": "rm -rf node_modules .expo android/app/build ios/build && pnpm install",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@expo-google-fonts/jetbrains-mono": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-firebase/analytics": "^23.7.0",
    "@react-native-firebase/app": "^23.7.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.9.1",
    "@react-navigation/native": "^7.1.8",
    "@sentry/cli": "^2.58.2",
    "@sentry/react-native": "7.2.0",
    "@supabase/supabase-js": "^2.86.0",
    "date-fns": "^4.1.0",
    "expo": "~54.0.27",
    "expo-apple-authentication": "~8.0.8",
    "expo-application": "^7.0.8",
    "expo-auth-session": "^7.0.10",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.11",
    "expo-dev-client": "~6.0.20",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.10",
    "expo-glass-effect": "~0.1.8",
    "expo-image": "~3.0.11",
    "expo-insights": "~0.10.8",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.17",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.6.0",
    "lucide-react-native": "^0.555.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.18.0",
    "react-native-svg": "^15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.17",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "^54.0.8",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
